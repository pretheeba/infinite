@model IEnumerable<NorthwindMVC.Customer>

@{
    ViewBag.Title = "CustomersInGermany";
}

<h2>Customers in Germany</h2>

<table>
    <tr>
        <th>Customer ID</th>
        <th>Company Name</th>
        <th>Contact Name</th>
        <th>City</th>
    </tr>

    @foreach (var customer in Model)
    {
<tr>
    <td>@customer.CustomerID</td>
    <td>@customer.CompanyName</td>
    <td>@customer.ContactName</td>
    <td>@customer.City</td>
</tr>}
</table>
